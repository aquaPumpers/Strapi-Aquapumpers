{
  "files.associations": {
    "*.mdx": "markdown"
  },
    "eslint.validate": [
      "javascript",
      "javascriptreact",
      "astro",
      "typescript",
      "typescriptreact"
    ],
    "prettier.documentSelectors": ["**/*.astro"],
    "[astro]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    }
  }